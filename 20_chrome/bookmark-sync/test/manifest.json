{
    "manifest_version": 2,
    "name": "ICQL Bookmark Sync",
    "description": "书签同步[ https://api.icql.work/bookmark ]",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "自动同步书签！"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "permissions": [
        "tabs",
        "bookmarks",
        "storage",
        "http://*/",
        "https://*/"
    ]
}